@model Person

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Info";
}
<table border="2" align="center" width="70%">
    <tr>
        <th Person Details</th>
    </tr>
   
    <tr>
        <th>Full Name :</th>
        <th>@Model.name</th>
    </tr>
     <tr>
        <th>Basic Salary :</th>
        <th>@Model.basicSalary</th>
    </tr>
    <tr>
        <th>Age :</th>
        <th>@Model.age</th>
    </tr>

</table>
